NOTE: Because of sharebuffers you have to recreate
      the temp file each time you run.
      The code updates the input.

/*                       _       _
| |_ ___ _ __ ___  _ __ | | __ _| |_ ___
| __/ _ \ `_ ` _ \| `_ \| |/ _` | __/ _ \
| ||  __/ | | | | | |_) | | (_| | ||  __/
 \__\___|_| |_| |_| .__/|_|\__,_|\__\___|
                  |_|
*/
filename tmp temp;
data _null_;
 file tmp;
 do lyn=2 to 20;
  put @2 '-- |----------+---------+---------+---------+---------+---------+---------+---------+---------+---------|';
  row=put(lyn, 2. -r);
  put @1 row $2.
   @5 '|          |         |         |         |         |         |         |         |         |         |';
 end;
  put @2 '-- |----------+---------+---------+---------+---------+---------+---------+---------+---------+---------|';
 stop;
run;quit;

/*__ _ _ _            _ _
 / _(_) | |   ___ ___| | |___
| |_| | | |  / __/ _ \ | / __|
|  _| | | | | (_|  __/ | \__ \
|_| |_|_|_|  \___\___|_|_|___/

*/
ata _null_;
  infile tmp sharebuffers;
  file tmp;
  input;
  put _infile_;
  input;
  set sd1.sheet2 point=_n_ nobs=numobs;
  put @6 DAYNUM @16 DAY @26 BREAKFAST @36 LUNCH;
  call symput('lines',2*_n_+1);
  if _n_=numobs then stop;
run;quit;
/*         _     _   _                    _           
  __ _  __| | __| | | |__   ___  __ _  __| | ___ _ __ 
 / _` |/ _` |/ _` | | `_ \ / _ \/ _` |/ _` |/ _ \ `__|
| (_| | (_| | (_| | | | | |  __/ (_| | (_| |  __/ |   
 \__,_|\__,_|\__,_| |_| |_|\___|\__,_|\__,_|\___|_|   
                                                      
*/
data _null_;                                                                                                             
  infile tmp;                                                                                                            
  input;                                                                                                                 
  if _n_=1 then do;                                                                                                      
     put "-----------------------+";                                                                                     
     put "| A1| fx    |DAYNUM    |";                                                                                     
     put "---------------------------------------------------------------------------------------------------------+";   
     put "[_] |    A     |    B    |    C    |    E    |    F    |    G    |    H    |    I    |    J    |    K    |";   
     put "---------------------------------------------------------------------------------------------------------|";   
     PUT " 1  | NAME     |   SEX   |   AGE   | HEIGHT  | WEIGHT  |         |         |         |         |         |";   
  end;                                                                                                                   
  putlog _infile_;                                                                                                       
  if _n_=&lines then do;                                                                                                 
     putlog '[CLASS}';                                                                                                   
     stop;                                                                                                               
  end;                                                                                                                   
run;quit;                                                                                                                

